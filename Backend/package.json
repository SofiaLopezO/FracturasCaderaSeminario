        "test:unit": "vitest run tests/ --exclude tests/integration/ --reporter=verbose",
        "coverage": "vitest run --coverage",
        "coverage:integration": "vitest run tests/integration/ --coverage",
        "seed:tipos": "node scripts/seed_tipos.js",
